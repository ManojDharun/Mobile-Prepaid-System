<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Plan">
   <children>
      <AnchorPane fx:id="apane" layoutX="-6.0" layoutY="-4.0" prefHeight="425.0" prefWidth="483.0" style="-fx-background-color: lightgray;">
         <children>
            <Button fx:id="get" layoutX="382.0" layoutY="84.0" mnemonicParsing="false" onAction="#getNow" prefHeight="25.0" prefWidth="90.0" style="-fx-background-color: violet;" text="GET NOW" />
            <TextField fx:id="txt" layoutX="378.0" layoutY="48.0" prefHeight="25.0" prefWidth="98.0" />
            <Button fx:id="View" layoutX="290.0" layoutY="14.0" mnemonicParsing="false" onAction="#view" prefHeight="25.0" prefWidth="183.0" style="-fx-background-color: lightgreen; -fx-border: 10;" text="View the already availed plans" />
            <DatePicker fx:id="date" layoutX="305.0" layoutY="116.0" promptText="Current Date" />
         </children></AnchorPane>
   </children>
</AnchorPane>
